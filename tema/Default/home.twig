                    <section id="commenti">
                        <textarea wrap="on" id="commento" name="commento" style="color: #777777;"></textarea>
                    </section>
                    <div id="smile" style="margin-top: -25px;"><center>
                            <a href="javascript:Smile(' :) ')"><img src="{{ root }}/images/smilies/sorriso_lingua.png" alt=":)" title=":)" border="0"></a>

                            <a href="javascript:Smile(' :0 ')"><img src="{{ root }}/images/smilies/sorrisone.png" alt=":0" title=":0" border="0"></a>
