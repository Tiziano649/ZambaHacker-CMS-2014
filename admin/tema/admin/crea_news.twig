<body>

<div class="container-fluid">

    <!-- Header starts -->
    <div class="row-fluid">
        <div class="span12">

        {% include 'header.twig' %}

            <!-- End header -->

        </div>

    </div>

    <!-- Header ends -->

    <div class="row-fluid">

        <!-- Sidebar statrt -->
        <div class="span2 sidebar-container">

            <div class="sidebar">

                <div class="navbar sidebar-toggle">
                    <div class="container">

                        <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </a>

                    </div>
                </div>

                {% include 'menu.twig' %}

            </div>
        </div>

        <!-- Sidebar end -->

        <!-- Body start -->
            <div class="span10 body-container">

                

                    <section id="formElement" class="utopia-widget utopia-form-box section">
    <div class="utopia-widget-title">
<img src="tema/admin/img/icons/window.png" class="utopia-widget-icon">
        <span>Crea una News</span>
    </div>

    <div class="row-fluid">
        <div class="utopia-widget-content">
            <div class="span6 utopia-form-freeSpace">
                <form class="form-horizontal">
                    <fieldset>
                        <div class="control-group">
                            <label class="control-label" for="input01">Titolo:</label>

                            <div class="controls">
                                <input id="titolo" class="span10" type="text"/>
                            </div>
                        </div>
                        
                        <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Descrizione:</label>

                            <div class="controls">
                               <textarea id="short" style="height: 76px;" class="span10"></textarea>
                        </div> </div>
                        
                          <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">News completa:</label>

                            <div class="controls">
                              <textarea id="long" style="height: 294px;" class="span10"></textarea>
                            </div>
                        </div>
                        
                        
                
                        
                    </fieldset>
                </form>
            </div>

            <div class="span6 utopia-form-freeSpace">
                <form class="form-horizontal" action="javascript:void(0)">
                    <fieldset>
                    
                            <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Immagine:</label>

                            <div class="controls"> 
                                <input  id="img" class="span10"  type="text">
                            </div>
                        </div>
                        
                        <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Autore:</label>

                            <div class="controls">
                                <input  id="autore" class="span10" value="{{ hk.utente.username }}" type="text">
                            </div>
                        </div>
                        
                    
                        
                        <div class="control-group1" style="margin-bottom: 18px;">


                            <div class="controls">
                            <button class="btn btn-large span5" style="margin-left: 0px;" onclick="javascript:crea_news();">Crea</button>
                            </div>
                            
                        </div>
                        
                    </fieldset>
                    
                    
                </form>
            </div>

        </div>
    </div><div class="utopia-widget-content utopia-form-freeSpace ">
                    <div id="msg_err" class="alert alert-error" style="display: none;">
                     
                        </div>
                         <div id="msg_s" class="alert alert-success" style="display: none;">  </div>
</section>
            
            
            
                        <script type="text/javascript">
            (function() {
                var asjs = document.createElement('script');
                asjs.type = 'text/javascript';
                asjs.async = true;
                asjs.src ='../js/admin/news.js';
                (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(asjs);
            }());
        </script>


                
