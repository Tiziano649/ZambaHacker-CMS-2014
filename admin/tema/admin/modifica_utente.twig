    <link href="tema/admin/css/ie.css" rel="stylesheet"/>

    <script type="text/javascript" src="tema/admin/admin_files/jquery.min.js"></script>
    <script type="text/javascript" src="tema/admin/admin_files/jquery.cookie.js"></script>


