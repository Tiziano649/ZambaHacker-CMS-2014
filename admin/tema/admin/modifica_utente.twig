<body>

<div class="container-fluid">

    <!-- Header starts -->
    <div class="row-fluid">
        <div class="span12">

        {% include 'header.twig' %}

            <!-- End header -->

        </div>

    </div>

    <!-- Header ends -->

    <div class="row-fluid">

        <!-- Sidebar statrt -->
        <div class="span2 sidebar-container">

            <div class="sidebar">

                <div class="navbar sidebar-toggle">
                    <div class="container">

                        <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </a>

                    </div>
                </div>

                {% include 'menu.twig' %}

            </div>
        </div>

        <!-- Sidebar end -->

        <!-- Body start -->
            <div class="span10 body-container">

                

                    <div class="row-fluid">
                <div class="span12">
                    <section class="utopia-widget">
                        <div class="utopia-widget-title">
                            <img src="tema/admin/img/icons/window.png" class="utopia-widget-icon">
                            <span>Cerca un utente</span>
                        </div>

                        <div class="utopia-widget-content utopia-form-freeSpace ">
                        <div id="centro">
                        <p style="margin-bottom: 60px;"><input class="span5" id="nome" type="text" placeholder="Nome utente"/>
                        <button class="btn btn-large span5" style="margin-left: 0px;" onclick="javascript:cerca();">Cerca</button></p>
                     <div id="msg_cerca" class="alert alert-error" style="display: none;">
                  
                    </div>
                        </div>
                        </div>
                    </section>
                </div>
            </div>
            
            {%if hk.user.NUM != 0 %}
            
            <section id="formElement" class="utopia-widget utopia-form-box section">
    <div class="utopia-widget-title">
<img src="tema/admin/img/icons/window.png" class="utopia-widget-icon">
        <span>Modifica {{ hk.user.username}}</span>
    </div>

    <div class="row-fluid">
        <div class="utopia-widget-content">
            <div class="span6 utopia-form-freeSpace">
                <form class="form-horizontal">
                    <fieldset>
                        <div class="control-group">
                            <label class="control-label" for="input01">Id:</label>

                            <div class="controls">
                                <input disabled="true" id="id" class="span10" value="{{ hk.user.id }}" type="text">
                            </div>
                        </div>
                        
                        <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Username:</label>

                            <div class="controls">
                                <input {%if hk.permessi.hk_username == 0 %}disabled="true"{%endif%} id="username" class="span10" value="{{ hk.user.username }}" type="text">
                                <input {%if hk.permessi.hk_username == 0 %}disabled="true"{%endif%} id="username1" class="span10" style="display:none;" value="{{ hk.user.username }}" type="text">
                            </div>
                        </div>
                        
                          <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Mail:</label>

                            <div class="controls">
                                <input {%if hk.permessi.hk_mail == 0 %}disabled="true"{%endif%} id="mail" class="span10" value="{{ hk.user.mail }}" type="text">
                            </div>
                        </div>
                        
                        <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Rank:</label>

                            <div class="controls">
                                <input {%if hk.permessi.hk_rank == 0 %}disabled="true"{%endif%} id="rank" class="span10" value="{{ hk.user.rank }}" type="text">
                            </div>
                        </div>
                        
                        <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Crediti:</label>

                            <div class="controls">
                                <input  {%if hk.permessi.hk_credits == 0 %}disabled="true"{%endif%} id="credits" class="span10" value="{{ hk.user.credits }}" type="text">
                            </div>
                        </div>
                        
                        <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Vip points:</label>

                            <div class="controls">
                                <input  {%if hk.permessi.hk_vip_points == 0 %}disabled="true"{%endif%} id="vip_points" class="span10" value="{{ hk.user.vip_points }}" type="text">
                            </div>
                        </div>
                        
                        <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Activity points:</label>

                            <div class="controls">
                                <input  {%if hk.permessi.hk_activity_points == 0 %}disabled="true"{%endif%} id="activity_points" class="span10" value="{{ hk.user.activity_points }}" type="text">
                            </div>
                        </div>
                        
                        <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Motto:</label>

                            <div class="controls">
                                <input  {%if hk.permessi.hk_motto == 0 %}disabled="true"{%endif%} id="motto" class="span10" value="{{ hk.user.motto }}" type="text">
                            </div>
                        </div>
                        
                
                        
                    </fieldset>
                </form>
            </div>

            <div class="span6 utopia-form-freeSpace">
                <form class="form-horizontal" action="javascript:void(0)">
                    <fieldset>
                    
                            <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Ruolo:</label>

                            <div class="controls"> 
                                <input  {%if hk.permessi.hk_ruolo == 0 %}disabled="true"{%endif%} id="ruolo" class="span10" value="{{ hk.user.ruolo }}" type="text">
                            </div>
                        </div>
                        
                        <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Ultima Visita:</label>

                            <div class="controls">
                                <input disabled="true" id="ultima_visita" class="span10" value="{{ hk.user.ultima_visita }}" type="text">
                            </div>
                        </div>
                        
                        <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Ip:</label>

                            <div class="controls">
                                <input disabled="true" id="ip" class="span10" value="{{ hk.user.ip_last}}" type="text">
                            </div>
                        </div>
                        
                        <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Rispetti:</label>

                            <div class="controls">
                                <input  {%if hk.permessi.hk_rispetti == 0 %}disabled="true"{%endif%} id="rispetti" class="span10" value="{{ hk.user.Respect}}" type="text">
                            </div>
                        </div>
                        
                        <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Tempo Online:</label>

                            <div class="controls">
                                <input  disabled="true" id="ontime" class="span10" value="{{ hk.user.OnlineTime}}" type="text">
                            </div>
                        </div>
                        
                        <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Rispetti Giornalieri:</label>

                            <div class="controls">
                                <input  {%if hk.permessi.hk_respectgg == 0 %}disabled="true"{%endif%} id="rixp_gg" class="span10" value="{{ hk.user.DailyRespectPoints}}" type="text">
                            </div>
                        </div>
                        
                        <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Grattini Giornalieri:</label>

                            <div class="controls">
                                <input  {%if hk.permessi.hk_grattini == 0 %}disabled="true"{%endif%} id="rixp_pt_gg" class="span10" value="{{ hk.user.DailyPetRespectPoints}}" type="text">
                            </div>
                        </div>
                        
                       <div class="control-group1" style="margin-bottom: 18px;">
                            <label class="control-label" for="input01">Id Gruppo:</label>

                            <div class="controls">
                                <input  {%if hk.permessi.hk_id_gruppo == 0 %}disabled="true"{%endif%} id="id_gruppo" class="span10" value="{{ hk.user.groupid}}" type="text">
                            </div>
                        </div>
                        
                        <div class="control-group1" style="margin-bottom: 18px;">


                            <div class="controls">
                            <button class="btn btn-large span5" style="margin-left: 0px;" onclick="javascript:modifica('{{ hk.user.username }}');">Modifica</button>
                            </div>
                            
                        </div>
                        
                    </fieldset>
                    
                    
                </form>
            </div>

        </div>
    </div><div class="utopia-widget-content utopia-form-freeSpace ">
                     <div id="msg_mod"></div>
                        
                        </div>
</section>
            
            
            {%endif%}
            
            
                        <script type="text/javascript">
            (function() {
                var asjs = document.createElement('script');
                asjs.type = 'text/javascript';
                asjs.async = true;
                asjs.src ='../js/admin/cerca.js';
                (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(asjs);
            }());
        </script>


                
            </div>
        <!-- Body end -->
        
        {% include 'footer.twig' %}

    </div>

    <!-- Maincontent end -->

</div> <!-- end of container -->

<!-- javascript placed at the end of the document so the pages load faster -->
<script type="text/javascript" src="tema/admin/admin_files/utopia.js"></script>
<script type="text/javascript" src="tema/admin/admin_files/jquery.hoverIntent.min.js"></script>
<script type="text/javascript" src="tema/admin/admin_files/jquery.easing.1.3.js"></script>
<script type="text/javascript" src="tema/admin/admin_files/jquery.datatable.js"></script>
<script type="text/javascript" src="tema/admin/admin_files/tables.js"></script>
<script type="text/javascript" src="tema/admin/admin_files/jquery.sparkline.js"></script>
<script type="text/javascript" src="tema/admin/admin_files/jquery.vticker-min.js"></script>
<script type="text/javascript" src="tema/admin/admin_files/datepicker.js"></script>
<script type="text/javascript" src="tema/admin/admin_files/load-image.min.js"></script>
<script type="text/javascript" src="tema/admin/admin_files/image-gallery.min.js"></script>
<script type="text/javascript" src="tema/admin/admin_files/jquery.simpleWeather.js"></script>
<script src="tema/admin/admin_files/jquery.validationEngine.js" type="text/javascript"></script>
<script type="text/javascript" src="tema/admin/admin_files/maskedinput.js"></script>
<script type="text/javascript" src="tema/admin/admin_files/chosen.jquery.js"></script>

<script type="text/javascript" src="tema/admin/admin_files/header.js"></script>
<script type="text/javascript" src="tema/admin/admin_files/sidebar.js"></script>




<div id="ui-datepicker-div" class="ui-datepicker ui-widget ui-widget-content ui-helper-clearfix ui-corner-all"></div></body></html>